{
  "name": "react-redux",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.0.0",
    "classnames": "^2.2.5",
    "date-fns": "^1.29.0",
    "highcharts": "^6.0.7",
    "moment": "^2.21.0",
    "normalizr": "^3.2.4",
    "react": "^16.2.0",
    "react-day-picker": "^7.1.4",
    "react-dom": "^16.2.0",
    "react-highcharts": "^16.0.2",
    "react-jsx-highcharts": "^2.2.1",
    "react-jsx-highstock": "^2.2.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "reactstrap": "^5.0.0-beta",
